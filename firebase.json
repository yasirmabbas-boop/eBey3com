{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "ebey3-backend",
          "region": "us-central1"
        }
      }
    ]
  }
}
